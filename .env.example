# NAAC AI Assistant Environment Variables
# Copy this file to .env and fill in your actual API keys

# IBM Cloud Services
REACT_APP_IBM_CLOUD_API_KEY=your_ibm_cloud_api_key_here
REACT_APP_IBM_GRANITE_MODEL_ID=ibm/granite-13b-chat-v2
REACT_APP_IBM_GRANITE_URL=https://us-south.ml.cloud.ibm.com/ml/v1/text/generation
REACT_APP_IBM_PROJECT_ID=your_ibm_project_id_here

# Pinecone Configuration
REACT_APP_PINECONE_API_KEY=your_pinecone_api_key_here
REACT_APP_PINECONE_ENVIRONMENT=us-east-1
REACT_APP_PINECONE_INDEX_NAME=naac-documents
REACT_APP_PINECONE_PROJECT_ID=your_pinecone_project_id_here

# Cohere for embeddings
REACT_APP_COHERE_API_KEY=your_cohere_api_key_here
