# IBM Cloud Services (Example Configuration)
# Copy this file to .env and replace with your actual API keys

# IBM Granite LLM
REACT_APP_IBM_CLOUD_API_KEY=your_ibm_api_key_here
REACT_APP_IBM_GRANITE_MODEL_ID=ibm/granite-13b-instruct-v2
REACT_APP_IBM_GRANITE_URL=https://eu-de.ml.cloud.ibm.com/ml/v1/text/generation?version=2023-05-29
REACT_APP_IBM_PROJECT_ID=your_project_id_here

# Pinecone Vector Database
REACT_APP_PINECONE_API_KEY=your_pinecone_api_key_here
REACT_APP_PINECONE_ENVIRONMENT=us-east-1
REACT_APP_PINECONE_INDEX_NAME=naac-documents
REACT_APP_PINECONE_PROJECT_ID=your_pinecone_project_id_here

# Cohere for embeddings
REACT_APP_COHERE_API_KEY=your_cohere_api_key_here

# Backend API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_API_BASE_URL_PRODUCTION=https://your-prod-host/api

# Application Settings
REACT_APP_APP_NAME=NAAC AI Assistant
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development
