# Environment Variables for NAAC AI Assistant

# IBM Cloud Services (MANDATORY)
REACT_APP_IBM_CLOUD_API_KEY=your_ibm_cloud_api_key
REACT_APP_IBM_GRANITE_MODEL_ID=ibm/granite-13b-chat-v2
REACT_APP_IBM_GRANITE_URL=https://us-south.ml.cloud.ibm.com
REACT_APP_IBM_PROJECT_ID=your_watson_ml_project_id

# Watson NLP (for document parsing)
REACT_APP_WATSON_NLP_API_KEY=your_watson_nlp_api_key
REACT_APP_WATSON_NLP_URL=https://api.us-south.natural-language-understanding.watson.cloud.ibm.com
REACT_APP_WATSON_NLP_VERSION=2022-04-07

# IBM Cloud Object Storage
REACT_APP_IBM_COS_API_KEY=your_cos_api_key
REACT_APP_IBM_COS_INSTANCE_CRN=your_cos_instance_crn
REACT_APP_IBM_COS_ENDPOINT=https://s3.us-south.cloud-object-storage.appdomain.cloud
REACT_APP_IBM_COS_BUCKET_NAME=naac-documents

# IBM App ID (Authentication)
REACT_APP_IBM_APP_ID_CLIENT_ID=your_app_id_client_id
REACT_APP_IBM_APP_ID_OAUTH_SERVER_URL=https://us-south.appid.cloud.ibm.com/oauth/v4/your-tenant-id

# Pinecone Vector Database (REPLACES Watson Discovery)
REACT_APP_PINECONE_API_KEY=your_pinecone_api_key
REACT_APP_PINECONE_ENVIRONMENT=your_pinecone_environment
REACT_APP_PINECONE_INDEX_NAME=naac-documents

# OpenAI (for embeddings - alternative to Watson Discovery)
REACT_APP_OPENAI_API_KEY=your_openai_api_key

# Cohere (alternative embedding provider)
REACT_APP_COHERE_API_KEY=your_cohere_api_key

# Backend API
REACT_APP_API_BASE_URL=http://localhost:8000/api

# Application Settings
REACT_APP_APP_NAME=NAAC AI Assistant
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development
